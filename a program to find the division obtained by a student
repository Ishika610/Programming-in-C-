#include <bits/stdc++.h>
using namespace std;
int main(int argc, char const *argv[])
{
    int per;
    cout<<"enter your percentage ";
    cin>>per;
    if (per>=60)
    {
        cout<<"1st division";
    }
    else if (per<60&&per>=50)
    {
        cout<<"2nd division";
    }
    else if (per<50&&per>=40)
    {
        cout<<"3rd division";
    }
    else
    {
        cout<<"fail";
    }
    
    
    
    return 0;
}
