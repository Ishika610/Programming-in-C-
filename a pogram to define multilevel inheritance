#include <bits/stdc++.h>
using namespace std;
class A
{
    public:
    A()
    {
        cout<<"this is base class constructor"<<endl;
    }
};
class B:public A
{
    public:
    B()
    {
        cout<<"this is derived class constructor"<<endl;
    }
};
class C: public B
{
    public:
    C()
    {
        cout<<"this is C class constructor"<<endl;
    }
};
int main()
{
    C obj;
    return 0;
}
