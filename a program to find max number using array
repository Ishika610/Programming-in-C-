#include <bits/stdc++.h>
using namespace std;

int main()
{
    int ar[8],i,max;
    cout<<"enter array";
    for(i=0;i<8;i++)
    {
        cin>>ar[i];
    }
    max=ar[0];
    for(i=1;i<8;i++)
    {
        if(ar[i]>max)
        {
            max=ar[i];
        }
    }
    cout<<max;
    return 0;
}
