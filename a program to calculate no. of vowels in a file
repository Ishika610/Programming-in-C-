#include <bits/stdc++.h>
using namespace std;

int main()
{
    char ch;
    int count=0;
    ifstream fin;
    fin.open("ab.cpp");
    while(fin)
    {
        fin>>ch;
        if(ch=='a'||ch=='A'||ch=='e'||ch=='E'||ch=='i'||ch=='I'||ch=='o'||ch=='O'||ch=='u'||ch=='U')
        {
            count++;
        }
        else
        {
            count=0;
        }
        

    }
    cout<<count;
    return 0;
}
