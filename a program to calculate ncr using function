#include <bits/stdc++.h>
using namespace std;
int fact(int n)
{
    int f=1;
    for(int i=n;i>=1;i--)
    {
        f=f*i;
        
    }
    return f;
}
int main()
{
    int n,ncr,r;
    cout<<"enter n and r";
    cin>>n>>r;
    ncr= fact(n)/(fact(n-r)*fact(r));
    cout<<"ncr is"<<ncr;
    return 0;
}
