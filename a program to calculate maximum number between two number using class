#include <bits/stdc++.h>
using namespace std;
class maximum
{
    int a,b;
    public:
    void getdata()
    {
        cout<<"enter two numbers";
        cin>>a>>b;
    }
    void process()
    {
        if(a>b)
        {
            cout<<a<<"is max";
        }
        else
        {
            cout<<b<<"is max";
        }
        
    }
};
int main()
{
    maximum m;
    m.getdata();
    m.process();
    return 0;
}
