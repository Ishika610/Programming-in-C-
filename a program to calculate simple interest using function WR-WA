#include <bits/stdc++.h>
using namespace std;
float si(int, float, int);
int main()
{
    int p, t;
    float s, r;
    cout << "enter p,r and t";
    cin >> p >> r >> t;
    s=si(p,r,t);
    cout<<s;
        return 0;
}
float si(int p,float r,int t)
{
    float s;
    s=(p*r*t)/100;
    return s;
}
