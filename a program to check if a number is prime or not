#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n, i;
    int count = 0;
    cout << "enter your number ";
    cin >> n;

    for (i = 2; i <= n / 2; i++)
    {
        if (n % i == 0)
        {
            count = 1;
        }
        else
            count = 0;
    }
    if (count == 1)
    {
        cout << "number is not prime";
    }
    else
    {
        cout << "number is prime";
    }

    return 0;
}
