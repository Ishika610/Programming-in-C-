#include <bits/stdc++.h>
  using namespace std;
  struct student
  {
      int rollno;
      char name[10];
      char sec;
  };
  int main()
  {
      student s[4];
      int i;
      for(i=0;i<4;i++)
      {
          cout<<"enter rollno";
          cin>>s[i].rollno;
          cout<<"enter name";
          cin>>s[i].name;
          cout<<"enter sec";
          cin>>s[i].sec;
          cout<<"rollno\t name\t sec\n";
      }
      for(i=0;i<4;i++)
      {
          if(s[i].sec=='a')
          {
              cout<<s[i].rollno<<"\t";
              cout<<s[i].name<<"\t";
              cout<<s[i].sec<<"\n";

          }
      }
      return 0;
  }
