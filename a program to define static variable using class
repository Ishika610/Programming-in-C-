#include <bits/stdc++.h>
using namespace std;
class test
{
    int a;
    static int b;
    public:
    test()
    {
        a=0;
    }
    void putdata()
    {
        a++;b++;
        cout<<"a="<<a<<endl;
        cout<<"b="<<b<<endl;
    }
};
int test::b;
int main()
{
    test t1;
    t1.putdata();
    t1.putdata();
    test t2;
    t2.putdata();
    t2.putdata();
    t1.putdata();
    test t3;
    t3.putdata();   
    return 0;
}
