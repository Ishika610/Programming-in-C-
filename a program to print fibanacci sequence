#include <bits/stdc++.h>
using namespace std;

void fib(int a)
{
    int s2;
    int s=0;
    int s1=1;
    
    for(int i=0;i<=a;i++)
    {
        cout<<s;
        s2=s+s1;
        s=s1;
        s1=s2;
    }
}
int main()
{
    int n;
    cout<<"enter a number";
    cin>>n;
    fib(n);
    return 0;
}
