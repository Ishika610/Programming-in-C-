#include <bits/stdc++.h>
using namespace std;
class test
{
    int a;
    public:
    void getdata(int x)
    {
        a=x;
    }
    void operator-()
    {
        a=-a;
    }
    void putdata()
    {
        cout<<a;
    }
};
int main()
{
    test s1;
    s1.getdata(10);
    -s1;
    s1.putdata();
    return 0;
}
