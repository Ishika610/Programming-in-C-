#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int ar[n];
    for(int i=0; i<n;i++)
    {
        cin>>ar[i];
    }
    for(int i=0;i<n;i++)
    {
        if(i==0)
        {
            cout<<ar[i]<<" ";
        }
        else
        {
            int z= max(ar[i],ar[i-1]);
            cout<<z<<" ";
        }
    }
    return 0;
}
