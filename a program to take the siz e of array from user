#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,i,*ar;
    cout<<"enter size of array";
    cin>>n;
    ar=new int[n];
    cout<<"enter number in array";
    for(i=0;i<n;i++)
    {
        cin>>ar[i];
    }
    for(i=0;i<n;i++)
    {
        cout<<ar[i]<<" ";
    }
     delete []ar;
    return 0;
}
